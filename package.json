{
  "name": "redux-example",
  "version": "1.0.0",
  "description": "Redux example app",
  "scripts": {
    "clean": "rimraf public",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.babel.js",
    "build": "npm run clean && npm run build:webpack",
    "preinstall": "npm prune",
    "postinstall": "npm run build",
    "start": "pm2 stop qurator; NODE_ENV=production pm2 start index.js --name qurator",
    "dev": "nodemon --watch server --watch node_modules .",
    "fixtures": "babel-node ./server/fixtures",
    "fixturesCSV": "babel-node ./server/fixtures/uploadCSV",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valerybugakov/redux-example"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Valery Bugakov (skymk1@gmail.com)",
  "homepage": "https://github.com/valerybugakov/redux-example",
