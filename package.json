{
  "name": "qurator",
  "version": "0.1.0",
  "description": "Qurator app",
  "scripts": {
    "test": "npm run lint",
    "clean": "rimraf public",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build": "npm run clean && npm run build:webpack",
    "preinstall": "npm prune",
    "postinstall": "npm run build",
    "start": "pm2 stop qurator; NODE_ENV=production pm2 start index.js --name qurator",
    "dev": "npm run clean && nodemon --watch server --watch node_modules .",
    "fixtures": "babel-node ./server/fixtures/uploadCSV",
    "fixturesShopify": "babel-node ./server/fixtures/shopify",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yangit/qutator"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Valery Bugakov (skymk1@gmail.com)",
